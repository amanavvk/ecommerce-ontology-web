{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "NEXT_PUBLIC_APP_ENV": "production",
    "FUSEKI_ENDPOINT": "/api/local-sparql",
    "NEXT_PUBLIC_FUSEKI_ENDPOINT": "/api/local-sparql"
  }
}
