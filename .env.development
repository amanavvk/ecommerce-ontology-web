# Development Environment Configuration

# Use mock data for development (set to false to use real Fuseki)
NODE_ENV=development
FORCE_REAL_SPARQL=false

# Fuseki SPARQL Endpoint (for when FORCE_REAL_SPARQL=true)
FUSEKI_ENDPOINT=http://localhost:3030/manufacturing/sparql
NEXT_PUBLIC_FUSEKI_ENDPOINT=http://localhost:3030/manufacturing/sparql

# MySQL Database Configuration (for development)
MYSQL_HOST=localhost
MYSQL_PORT=3306
MYSQL_DATABASE=manufacturing
MYSQL_USER=r2rml_user
MYSQL_PASSWORD=secure_password

# Development Settings
LOG_LEVEL=debug
ENABLE_QUERY_LOGGING=true
